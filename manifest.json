{
	"name": "NPELS 课程测试辅助",
	"version": "1.3",
	"short_name": "npels-helper",
	"description": "新理念外语网络教学中心 课程在线测试 辅助扩展程序",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "img/logo48.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"48": "img/logo48.png",
		"128": "img/logo128.png"
	},
	"content_scripts": [{
		"js": [
			"npels.js"
		],
		"matches": [
			"<all_urls>"
		]
	}],
	"permissions": [
		"activeTab"
	]
}
